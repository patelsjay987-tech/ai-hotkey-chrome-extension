{
  "manifest_version": 3,
  "name": "Academic AI Center",
  "version": "1.2",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["https://api.groq.com/*"],
  "web_accessible_resources": [{"resources": ["logo.png"], "matches": ["<all_urls>"]}],
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["script.js"],
    "css": ["style.css"]
  }]
}
